    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.batteriesViews.NewBatteryActivity" />
        <activity android:name=".view.surfersViews.EditSurferActivity" />
        <activity android:name=".view.surfersViews.NewSurferActivity" />
        <activity android:name=".view.wavesViews.NewWaveActivity" />
        <activity android:name=".view.wavesViews.RegisterWaveActivity" />
        <activity android:name=".view.notesViews.NewPunctuacionActivity" />
        <activity android:name=".view.surfersViews.SurfersListActivity"></activity>
    </application>

</manifest>