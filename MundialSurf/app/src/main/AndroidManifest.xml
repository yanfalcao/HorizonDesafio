<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.yanfalcao.mundialsurf">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".core.main.view.MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".core.hitCreation.view.hitCreationActivity" />
        <activity android:name=".core.surferEdition.view.EditSurferActivity" />
        <activity android:name=".core.surferCreation.view.NewSurferActivity" />
        <activity android:name=".core.hit.view.HitActivity" />
        <activity android:name=".core.scoreCreation.view.RegisterWaveActivity" />
        <activity android:name=".core.surfer.view.SurfersListActivity" />
    </application>

</manifest>